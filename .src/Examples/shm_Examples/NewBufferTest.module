' Gambas module file

Public Sub main()

  Dim MyMem As New ShmMem

  With MyMem
    Dim r As ShmBuffer = New ShmBuffer(MyMem, 200)
    Print r.Count
    Print r.Fields
    r.Define("F1", 20)
    r.Define("f2", 20)
    For Each s As ShmBufferField In r.Fields
      Print "Field = "; s.Name
    Next
  End With

  Quit 0

End
